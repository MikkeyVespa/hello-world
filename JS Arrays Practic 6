// BEGIN (write your solution here)
const calculateSum = (array) => {

  let countSum = 0;
  for (let i = 0; i < array.length; i++) {
    if (array[i] % 3 === 0) {
      countSum += array[i];

    }
  }
  return countSum;
}

export default calculateSum;
// END



teacher's decision

// BEGIN
const calculateSum = (coll) => {
  let sum = 0;
  for (let i = 0; i < coll.length; i += 1) {
    const value = coll[i];
    if (value % 3 === 0) {
      sum += value;
    }
  }

  return sum;
};

export default calculateSum;
// END
