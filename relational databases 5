CREATE TABLE users (
    id bigint PRIMARY KEY,
    first_name varchar(50),
    created_at timestamp
);

INSERT INTO users VALUES (1, 'Tom', '2012-10-1');

CREATE TABLE orders (
    id bigint PRIMARY KEY,
    user_first_name varchar(50),
    months int,
    created_at timestamp
);

INSERT INTO orders VALUES (1, 'Tom', 3, '2012-10-1');
INSERT INTO orders VALUES (2, 'Tom', 4, '2012-10-1');
--END
