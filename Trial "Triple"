export const make = (x, y, z) => (
  (word) => {
    switch (word) {
      case 'get1':
        return x;
      case 'get2':
        return y;
      case 'get3':
        return z;
    }
  }
);
export const get1 = (triple) => triple('get1');
export const get2 = (triple) => triple('get2');
export const get3 = (triple) => triple('get3');
